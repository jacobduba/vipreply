<tr id="<%= "example-#{example.id}" %>" class="odd:bg-white even:bg-gray-50 border-b">
  <td class="px-6 py-4 text-gray-700">
    <%= example.input %>
  </td>
  <td class="px-6 py-4 text-gray-700">
    <%= example.output %>
  </td>
  <td class="px-6 py-4 text-right">
    <%= link_to "Edit", edit_collection_example_path(@collection, example), data: {turbo_frame: "edit_example_modal"}, class: "font-medium text-blue-600 hover:underline" %>
  </td>
</tr>